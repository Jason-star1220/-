
#include"stdio.h"
#include"string.h"
int main()
{
	char suanshi[]="2*2/4*1/1*2*3/2";
	int res=suanshi[0]-'0';
	int a;
	
	for(int i=1;i<strlen(suanshi);i++)
	{
		if(suanshi[i]=='*')
		{
			a=suanshi[i+1]-'0';
			res=res*a;
			i++;
		}
		if(suanshi[i]=='/')
		{
			a=suanshi[i+1]-'0';
			res=res/a;
			i++;
		}
	}
	printf("%d\n",res);
	return 0;
}
